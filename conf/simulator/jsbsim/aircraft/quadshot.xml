<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="http://jsbsim.sourceforge.net/JSBSim.xsl"?>
<fdm_config name="QUAD COMPLETE EXT" version="2.0" release="BETA" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://jsbsim.sourceforge.net/JSBSim.xsd">

  <fileheader>
    <author>Koen Engelen</author>
    <filecreationdate>28-01-2021</filecreationdate>
    <version>Version 0.1 - beta</version>
    <description>Quadshot</description>
  </fileheader>

  <metrics>  <!-- Wing Information -->
    <wingarea unit="IN2"> 115 </wingarea> <!-- Ask Ewoud how he got to this value --> <!-- old value-->
    <wingspan unit="IN"> 39.37 </wingspan>
    <chord unit="IN"> 6.40 </chord>
    <htailarea unit="FT2"> 0 </htailarea>
    <htailarm unit="FT"> 0 </htailarm>
    <vtailarea unit="FT2"> 0 </vtailarea>
    <vtailarm unit="FT"> 0 </vtailarm>
    <wing_incidence unit="DEG"> 90 </wing_incidence>
    <location name="AERORP" unit="IN">
      <x> 0 </x>
      <y> 0 </y>
      <z> 0 </z>
    </location>
    <location name="EYEPOINT" unit="IN">
      <x> 0 </x>
      <y> 0 </y>
      <z> 0 </z>
    </location>
    <location name="VRP" unit="IN">
      <x> 0 </x>
      <y> 0 </y>
      <z> 0 </z>
    </location>
  </metrics>

  <mass_balance> <!-- Inertia and CG -->
    <ixx unit="SLUG*FT2"> 0.0132 </ixx> <!-- old value-->
    <iyy unit="SLUG*FT2"> 0.00250 </iyy> <!-- old value-->
    <izz unit="SLUG*FT2"> 0.0150 </izz> <!-- old value-->
    <ixy unit="SLUG*FT2"> 0. </ixy>
    <ixz unit="SLUG*FT2"> 0. </ixz>
    <iyz unit="SLUG*FT2"> 0. </iyz>
    <emptywt unit="LBS"> 2.2 </emptywt>
    <location name="CG" unit="M">
      <x> 0 </x>
      <y> 0 </y>
      <z> 0 </z>
    </location>
  </mass_balance>

  <ground_reactions> <!-- Collisions with Ground --> <!-- old values--> 
    <contact type="STRUCTURE" name="CONTACT_FRONT">
      <location unit="M">
        <x>-0.15 </x>
        <y> 0 </y>
        <z>-0.1 </z>
      </location>
      <static_friction>  0.8 </static_friction>
      <dynamic_friction> 0.5 </dynamic_friction>
      <spring_coeff unit="N/M"> 500 </spring_coeff>
      <damping_coeff unit="N/M/SEC"> 100 </damping_coeff>
      <damping_coeff_rebound type="SQUARE" unit="N/M2/SEC2"> 1000 </damping_coeff_rebound>
      <max_steer unit="DEG"> 0.0 </max_steer>
      <brake_group> NONE </brake_group>
      <retractable>0</retractable>
    </contact>

    <contact type="STRUCTURE" name="CONTACT_BACK">
      <location unit="M">
        <x> 0.15</x>
        <y> 0   </y>
        <z>-0.1 </z>
      </location>
      <static_friction>  0.8 </static_friction>
      <dynamic_friction> 0.5 </dynamic_friction>
      <spring_coeff unit="N/M"> 500 </spring_coeff>
      <damping_coeff unit="N/M/SEC"> 100 </damping_coeff>
      <damping_coeff_rebound type="SQUARE" unit="N/M2/SEC2"> 1000 </damping_coeff_rebound>
      <max_steer unit="DEG"> 0.0 </max_steer>
      <brake_group> NONE </brake_group>
      <retractable>0</retractable>
    </contact>

    <contact type="STRUCTURE" name="CONTACT_RIGHT">
      <location unit="M">
        <x> 0.  </x>
        <y> 0.15</y>
        <z>-0.1 </z>
      </location>
      <static_friction>  0.8 </static_friction>
      <dynamic_friction> 0.5 </dynamic_friction>
      <spring_coeff unit="N/M"> 500 </spring_coeff>
      <damping_coeff unit="N/M/SEC"> 100 </damping_coeff>
      <damping_coeff_rebound type="SQUARE" unit="N/M2/SEC2"> 1000 </damping_coeff_rebound>
      <max_steer unit="DEG"> 0.0 </max_steer>
      <brake_group> NONE </brake_group>
      <retractable>0</retractable>
    </contact>

    <contact type="STRUCTURE" name="CONTACT_LEFT">
      <location unit="M">
        <x> 0.  </x>
        <y>-0.15</y>
        <z>-0.1 </z>
      </location>
      <static_friction>  0.8 </static_friction>
      <dynamic_friction> 0.5 </dynamic_friction>
      <spring_coeff unit="N/M"> 500 </spring_coeff>
      <damping_coeff unit="N/M/SEC"> 100 </damping_coeff>
      <damping_coeff_rebound type="SQUARE" unit="N/M2/SEC2"> 1000 </damping_coeff_rebound>
      <max_steer unit="DEG"> 0.0 </max_steer>
      <brake_group> NONE </brake_group>
      <retractable>0</retractable>
    </contact>
  </ground_reactions>

  <flight_control name="actuator_dynamics"> <!-- Delays in Actuator Dynamics -->
    <channel name="filtering">

      <!--First order filter represents actuator dynamics-->
      <lag_filter name="ele_left_lag">
        <input> fcs/ele_left </input>
        <c1> 54 </c1> <!-- old value-->
        <output> fcs/ele_left_lag</output>
      </lag_filter>

      <lag_filter name="ele_right_lag">
        <input> fcs/ele_right </input>
        <c1> 54 </c1> <!-- old value-->
        <output> fcs/ele_right_lag</output>
      </lag_filter>
      
      <lag_filter name="mot_A1_lag">
        <input> fcs/mot_A1 </input>
        <c1> 31.7 </c1> <!-- old value-->
        <output> fcs/mot_A1_lag</output>
      </lag_filter>

      <lag_filter name="mot_A2_lag">
        <input> fcs/mot_A2 </input>
        <c1> 31.7 </c1> <!-- old value-->
        <output> fcs/mot_A2_lag</output>
      </lag_filter>

      <lag_filter name="mot_B1_lag">
        <input> fcs/mot_B1 </input>
        <c1> 31.7 </c1> <!-- old value-->
        <output> fcs/mot_B1_lag</output>
      </lag_filter>

      <lag_filter name="mot_B2_lag">
        <input> fcs/mot_B2 </input>
        <c1> 31.7 </c1> <!-- old value-->
        <output> fcs/mot_B2_lag</output>
      </lag_filter>

    </channel>
  </flight_control>

  <external_reactions> <!-- Addition of Thrust Forces -->

    <property>fcs/ele_left</property>
    <property>fcs/ele_left_lag</property>
    <property>fcs/ele_right</property>
    <property>fcs/ele_right_lag</property>
    <property>fcs/mot_A1</property>
    <property>fcs/mot_A1_lag</property>
    <property>fcs/mot_A2</property>
    <property>fcs/mot_A2_lag</property>
    <property>fcs/mot_B1</property>
    <property>fcs/mot_B1_lag</property>
    <property>fcs/mot_B2</property>
    <property>fcs/mot_B2_lag</property>


    <!-- First the lift forces produced by each propeller -->

    <force name="mot_A1" frame="BODY" unit="LBS">
      <function>
        <product>
          <property>fcs/mot_A1_lag</property>
          <value> 1.1 </value> <!-- old value-->
        </product>
      </function>
      <location unit="IN">
        <x>-3.09</x>
        <y>-5.45</y>
        <z> 0   </z>
      </location>
      <direction>
        <x> 0 </x>
        <y> 0 </y>
        <z>-1 </z>
      </direction>
    </force>

    <force name="mot_A2" frame="BODY" unit="LBS">
      <function>
        <product>
          <property>fcs/mot_A2_lag</property>
          <value> 1.1 </value> <!-- old value-->
        </product>
      </function>
      <location unit="IN">
        <x>-3.09</x>
        <y> 5.45</y>
        <z> 0   </z>
      </location>
      <direction>
        <x> 0 </x>
        <y> 0 </y>
        <z>-1 </z>
      </direction>
    </force>

    <force name="mot_B1" frame="BODY" unit="LBS">
      <function>
        <product>
          <property>fcs/mot_B1_lag</property>
          <value> 1.1 </value> <!-- old value-->
        </product>
      </function>
      <location unit="IN">
        <x>  3.09</x>
        <y>-12.93</y>
        <z>  0   </z>
      </location>
      <direction>
        <x> 0</x>
        <y> 0</y>
        <z>-1</z>
      </direction>
    </force>

    <force name="mot_B2" frame="BODY" unit="LBS">
      <function>
        <product>
          <property>fcs/mot_B2_lag</property>
          <value> 1.1 </value> <!-- old value-->
        </product>
      </function>
      <location unit="IN">
        <x> 3.09</x>
        <y>12.93</y>
        <z> 0   </z>
      </location>
      <direction>
        <x> 0</x>
        <y> 0</y>
        <z>-1</z>
      </direction>
    </force>


    <!-- Second the Moment arm produced by all propellers -->


    <force name="mom_arm_z1" frame="BODY" unit="LBS">
      <function>
        <sum>
          <product>
            <sum>
              <property>fcs/mot_B1_lag</property>
              <property>fcs/mot_A2_lag</property>
            </sum>
            <value> 0.35 </value> <!-- old value-->
          </product>
          <product>
            <sum>
              <property>fcs/mot_A1_lag</property>
              <property>fcs/mot_B2_lag</property>
            </sum>
            <value> -0.35 </value> <!-- old value-->
          </product>
        </sum>
      </function>
      <location unit="IN">
        <!-- Necessary arm in IN to produce a moment ten times
        "weaker" then the force when both are measured in the SI is 1.9685 in. -->
        <x>0</x>
        <y>6.8385</y>
        <z>0</z>
      </location>
      <direction>
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </direction>
    </force>

    <force name="mom_arm_z2" frame="BODY" unit="LBS">
      <function>
        <sum>
          <product>
            <sum>
              <property>fcs/mot_B1_lag</property>
              <property>fcs/mot_A2_lag</property>
            </sum>
            <value> 0.35 </value>
          </product>
          <product>
            <sum>
              <property>fcs/mot_A1_lag</property>
              <property>fcs/mot_B2_lag</property>
            </sum>
            <value> -0.35 </value>
          </product>
        </sum>
      </function>
      <location unit="IN">
        <!-- Necessary arm in IN to produce a moment ten times
        "weaker" then the force when both are measured in the SI is 1.9685 in. -->
        <x>0</x>
        <y>-6.8385</y>
        <z>0</z>
      </location>
      <direction>
        <x>1</x>
        <y>0</y>
        <z>0</z>
      </direction>
    </force>

  </external_reactions>

  <propulsion/>

  <flight_control name="FGFCS"/> <!--What is this linked to?-->

  <aerodynamics file="Systems/aerodynamics_quadshot.xml"/> <!-- Forces and Moments due to Aerodynamic Surfaces -->

</fdm_config>
